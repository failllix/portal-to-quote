{
  "name": "portal-to-quote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently --names 'supabase,services' 'npm run supabase' 'npm run services' ",
    "dev-backend": "nodemon --watch ./backend --watch ./shared --ext ts --exec tsx ./backend/main.ts",
    "dev-frontend": "next dev",
    "format": "biome format --write",
    "lint": "biome check",
    "services": "docker compose up --build",
    "start": "next start",
    "supabase": "supabase start",
    "supabase-seed": "supabase seed buckets",
    "db-update": "drizzle-kit push && tsx ./db/seed.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.90.1",
    "@ts-rest/core": "3.52.1",
    "@ts-rest/express": "3.52.1",
    "body-parser": "2.2.2",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.45.1",
    "drizzle-seed": "0.3.1",
    "express": "4.22.1",
    "next": "16.1.2",
    "postgres": "3.4.8",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/multer": "2.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "9.2.1",
    "drizzle-kit": "0.31.8",
    "nodemon": "3.1.11",
    "supabase": "2.72.7",
    "tailwindcss": "^4",
    "tsx": "4.21.0",
    "typescript": "^5",
    "zod": "3.25.76"
  }
}
