meta {
  name: Start File Processing
  type: http
  seq: 1
}

post {
  url: {{base_url}}/api/files/startProcessing
  body: json
  auth: inherit
}

body:json {
  {
    "id": "{{$randomUUID}}",
    "mimeType": "text/plain",
    "originalName": "dummy-file.step",
    "sizeBytes": 18000,
    "storagePath": "stepFiles/{{$randomUUID}}"
  }
}

vars:post-response {
  file_id: res.body.id
}
